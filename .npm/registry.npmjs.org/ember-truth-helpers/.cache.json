lver":"^2.0.3","ember-disable-prototype-extensions":"^1.0.0","ember-try":"~0.0.8"},"keywords":["ember-addon"],"dependencies":{"ember-cli-babel":"^5.1.5"},"ember-addon":{"configPath":"tests/dummy/config"},"gitHead":"fe86546b261d5c8ad7c21c9c8de34a62790b1330","bugs":{"url":"https://github.com/jmurphyau/ember-truth-helpers/issues"},"homepage":"https://github.com/jmurphyau/ember-truth-helpers","_id":"ember-truth-helpers@1.2.0","_shasum":"e63cffeaa8211882ae61a958816fded3790d065b","_from":".","_npmVersion":"2.5.1","_nodeVersion":"0.12.1","_npmUser":{"name":"jmurphyau","email":"jamesmurphyau@me.com"},"maintainers":[{"name":"jmurphyau","email":"jamesmurphyau@me.com"}],"dist":{"shasum":"e63cffeaa8211882ae61a958816fded3790d065b","tarball":"https://registry.npmjs.org/ember-truth-helpers/-/ember-truth-helpers-1.2.0.tgz"}}},"readme":"# Ember Truth Helpers [![Build Status](https://travis-ci.org/jmurphyau/ember-truth-helpers.svg?branch=master)](https://travis-ci.org/jmurphyau/ember-truth-helpers)\n\nHTMLBars template helpers for additional truth logic in `if` and `unless` statements.\n\n## Usage\n\nHelper   | JavaScript                           | HTMLBars\n---------|--------------------------------------|-------------------\neq       | `if (a === b)`                       | `{{if (eq a b)}}`\nnot-eq   | `if (a !== b)`                       | `{{if (not-eq a b)}}`\nnot      | `if (!a)`                            | `{{if (not a)}}`\nand      | `if (a && b)`                        | `{{if (and a b)}}`\nor       | <code>if (a &#124;&#124; b)</code>   | `{{if (or a b)}}`\nxor      | `if (a && !b || !a && b)`            | `{{if (xor a b)}}`\ngt       | `if (a > b)`                         | `{{if (gt a b)}}`\ngte      | `if (a >= b)`                        | `{{if (gte a b)}}`\nlt       | `if (a < b)`                         | `{{if (lt a b)}}`\nlte      | `if (a <= b)`                        | `{{if (lte a b)}}`\nis-array | `if (Ember.isArray(a))`              | `{{if (is-array a)}}`\n\n## Install\n\n* `ember install ember-truth-helpers`\n\n## Other Helpers\n\n* [ember-get-helper](https://github.com/jmurphyau/ember-get-helper)\n\n## Development\n\n* `git clone https://github.com/jmurphyau/ember-truth-helpers.git`\n* `npm install`\n* `bower install`\n* `ember server`\n* Visit your app at http://localhost:4200.\n\n## Running Tests\n\n* `npm test` (Runs `ember try:testall` to test your addon against multiple Ember versions)\n* `ember test`\n* `ember test --server`\n\n## Building\n\n* `ember build`\n\nFor more information on using ember-cli, visit [http://www.ember-cli.com/](http://www.ember-cli.com/).\n","maintainers":[{"name":"jmurphyau","email":"jamesmurphyau@me.com"}],"time":{"modified":"2015-10-23T20:13:56.014Z","created":"2015-01-08T23:18:40.521Z","0.0.1":"2015-01-08T23:18:40.521Z","0.0.2":"2015-01-13T06:22:28.623Z","0.0.3":"2015-02-10T02:55:21.349Z","0.0.4":"2015-02-26T06:39:32.503Z","0.0.5":"2015-03-17T10:18:21.766Z","1.0.0":"2015-07-06T03:09:08.010Z","1.1.0":"2015-08-10T02:28:21.666Z","1.2.0":"2015-10-23T20:13:56.014Z"},"homepage":"https://github.com/jmurphyau/ember-truth-helpers","keywords":["ember-addon"],"repository":{"type":"git","url":"https://github.com/jmurphyau/ember-truth-helpers"},"author":{"name":"James Murphy"},"bugs":{"url":"https://github.com/jmurphyau/ember-truth-helpers/issues"},"license":"MIT","readmeFilename":"README.md","users":{"knownasilya":true,"antramm":true,"dansh":true,"panman8201":true,"supairish":true,"shahrukhomar":true,"darrylr":true,"codebruder":true,"bmx269":true,"mswanson1524":true,"mpxy":true,"avaz":true,"algodave":true,"bmeurant":true,"joeperales":true,"alsemyonov":true,"dubbya":true},"_attachments":{},"_etag":"\"B6N4K2SNZU2X4BG0KL40P9GUY\""}